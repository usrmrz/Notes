<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Notes" parent="android:Theme.Material.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="android:colorSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/dark_gray</item>
        <!-- Customize your theme here. -->


    </style>

    <style name="Theme.Notes.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.NotesTodoAppPrep.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.NotesTodoAppPrep.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>